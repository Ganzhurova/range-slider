{
  "parser": "@babel/eslint-parser",
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:jest/recommended"],
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "no-console": "off"
  }
}
